<Defs>
	<!-- ==================== Pistol Ammo ========================== -->

	
	
	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_9x19mmPara_JHP_AAAM</defName>
		<label>make 9x19mm Parabellum (+p+) cartridge x100</label>
		<description>Craft 100 9x19mm Parabellum (+p+) cartridges.</description>
		<jobString>Making 9x19mm Parabellum (+p+) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
						<li>FSX</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
				<li>FSX</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_9x19mmPara_JHP>100</Ammo_9x19mmPara_JHP>
		</products>
		<researchPrerequisite>Pistols_C2</researchPrerequisite>
	</RecipeDef>
	
