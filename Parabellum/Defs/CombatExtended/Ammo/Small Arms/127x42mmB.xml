	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_127x42mmB_FMJ_AAAM</defName>
		<label>make .50 Beowulf (FMJ) cartridge x80</label>
		<description>Craft 80 .50 Beowulf(FMJ) cartridges.</description>
		<jobString>Making .50 Beowulf (FMJ) cartridges.</jobString>
		<targetCountAdjustment>80</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>16</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>8</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
						<li>FSX</li>
					</thingDefs>
				</filter>
				<count>6</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
				<li>FSX</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_127x42mmB_FMJ>80</Ammo_127x42mmB_FMJ>
		</products>

		<researchPrerequisite>Range_C2</researchPrerequisite>
	</RecipeDef>

