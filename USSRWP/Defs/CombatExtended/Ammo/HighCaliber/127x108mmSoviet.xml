	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_127x108mm_FMJ_AAAM</defName>
		<label>make 12.7x108mm (FMJ) cartridge x60</label>
		<description>Craft 60 12.7x108mm (FMJ) cartridges.</description>
		<jobString>Making 12.7x108mm (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>19</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>9</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_127x108mm_FMJ>60</Ammo_127x108mm_FMJ>
		</products>
		<researchPrerequisite>Sniper_rifles_C1</researchPrerequisite>
	</RecipeDef>

