	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_127x55mmAR_FMJ_AAAM</defName>
		<label>make 12.7x55mm (FMJ) cartridge x60</label>
		<description>Craft 60 12.7x55mm (FMJ) cartridges.</description>
		<jobString>Making 12.7x55mm (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>13</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>6</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_127x55mmAR_FMJ>60</Ammo_127x55mmAR_FMJ>
		</products>
		<researchPrerequisite>Range_C2</researchPrerequisite>
	</RecipeDef>

