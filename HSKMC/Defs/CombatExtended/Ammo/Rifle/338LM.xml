	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_338LM_FMJ_AAAM</defName>
		<label>make .338 Lapua Magnum (FMJ) cartridge x50</label>
		<description>Craft 50 .338 Lapua Magnum (FMJ) cartridges.</description>
		<jobString>Making .338 Lapua Magnum (FMJ) cartridges.</jobString>
		<targetCountAdjustment>50</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>14</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_338LM_FMJ>50</Ammo_338LM_FMJ>
		</products>

		<researchPrerequisite>Range_C2</researchPrerequisite>
	</RecipeDef>

