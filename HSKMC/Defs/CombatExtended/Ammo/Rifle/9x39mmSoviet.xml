	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
        <defName>MakeAmmo_9x39mmSoviet_FMJ_AAAM</defName>
        <label>make 9x39mm Soviet (SP-5) cartridge x90</label>
        <description>Craft 90 9x39mm Soviet (SP-5) cartridges.</description>
        <jobString>Making 9x39mm Soviet (SP-5) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>11</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_9x39mmSoviet_FMJ>90</Ammo_9x39mmSoviet_FMJ>
		</products>
	
	</RecipeDef>

