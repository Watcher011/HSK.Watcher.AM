	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_408CT_FMJ_AAAM</defName>
		<label>make .408 Chey Tac (FMJ) cartridge x50</label>
		<description>Craft 50 .408 Chey Tac (FMJ) cartridges.</description>
		<jobString>Making .408 Chey Tac (FMJ) cartridges.</jobString>
		<targetCountAdjustment>50</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>16</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>7</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_408CT_FMJ>50</Ammo_408CT_FMJ>
		</products>
	
		<researchPrerequisite>Range_C2</researchPrerequisite>
	</RecipeDef>

