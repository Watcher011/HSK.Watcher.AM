	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_545x39mmSoviet_FMJ_AAAM</defName>
		<label>make 5.45x39mm (FMJ) cartridge x80</label>
		<description>Craft 80 5.45x39mm (FMJ) cartridges.</description>
		<jobString>Making 5.45x39mm (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>7</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>HeavyBar</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
				<li>HeavyBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_545x39mmSoviet_FMJ>80</Ammo_545x39mmSoviet_FMJ>
		</products>
	
	</RecipeDef>

