	<RecipeDef ParentName="Make_AAAMAmmoBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>MakeAmmo_473x33mmCaseless_FMJ_AAAM</defName>
		<label>make 4.73x33mm Caseless (FMJ) cartridge x96</label>
		<description>Craft 96 4.73x33mm Caseless (FMJ) cartridges.</description>
		<jobString>Making 4.73x33mm Caseless (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>6</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plastic</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Powder</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
				<li>Plastic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_473x33mmCaseless_FMJ>96</Ammo_473x33mmCaseless_FMJ>
		</products>
		<researchPrerequisite>Range_C2</researchPrerequisite>
	</RecipeDef>

